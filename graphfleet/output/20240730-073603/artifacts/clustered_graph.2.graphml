    <edge source="&quot;GRAPH RAG&quot;" target="&quot;GRAPH COMMUNITIES&quot;">
      <data key="d9">1.0</data>
      <data key="d10">"Graph RAG uses graph communities to organize and summarize related entities within a text corpus."</data>
      <data key="d11">e7e620f804861b86c33f80a0f61ebb8c</data>
      <data key="d12">fb61c68efe5b4d69a9623e531e7c639c</data>
